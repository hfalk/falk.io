extends layout

block extraHeadScripts
    script(src='/javascript/cv.js')

block content
    header#cvHeader
        .col-md-4.col-md-offset-4
            img#falkPic.img-circle.img-thumbnail(src='images/falk.jpg', alt='')
        .col-md-4.col-md-offset-4
            a#socialLink(href='https://plus.google.com/u/0/+HåvardFalk')
                .google.contactIcon
            a#socialLink(href='https://github.com/hfalk')
                .github.contactIcon
            a#socialLink(href='https://www.linkedin.com/in/havardfalk')
                .linkedin.contactIcon
            a#socialLink(href='https://www.facebook.com/hfalk4')
                .facebook.contactIcon

    #nav
        nav.navbar.navbar-custom(role='navigation')
            .container
                .row
                    .col-md-12
                        // Brand and toggle get grouped for better mobile display
                        .navbar-header
                            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
                                i.fa.fa-bars
                        // Collect the nav links, forms, and other content for toggling
                        .collapse.navbar-collapse
                            ul.nav.navbar-nav
                                li
                                    a(href='#about') About
                                li
                                    a(href='#experience') Experience
                                li
                                    a(href='#education') Education
                                li
                                    a(href='#friviligeverv') Frivilige Verv
                                li
                                    a(href='#fritidsinteresser') Fritidsinteresser

    #about(name='about')
        .container
            .row
                .col-lg-2.col-lg-offset-1
                    h5.section-title About
                .col-lg-6
                    p Effektiv, imøtekommende, godt humør og fleksibel. Erfaring med lederarbeid og teamjobbing fra utdanning, militærtjeneste og frivillige verv. Programmeringskunnskaper innenfor Java/Andoid og Python.
                .col-lg-3
                    p
                        a.btn.btn-default.btn-lg(href='#', data-toggle='modal')
                            i.fa.fa-download.fa-lg
                            |       Download PDF

