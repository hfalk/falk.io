doctype html
html
    head
        title #{title}
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='description', content='')

        block headScripts
            link(href='/bootstrap33/css/bootstrap.min.css', rel='stylesheet')
            script(src='/javascript/jquery-2.1.4.min.js')
            link(href='/css/main.min.css', rel='stylesheet')


        block extraHeadScripts

    body
        block content






        //- Bootstrap core JavaScript
        //- block to load extra js in child templates
        block javaScripts
            //- Placed at the end of the document so the pages load faster
            script(src='/bootstrap33/js/bootstrap.min.js')

        block extra_js

        //- Google analytics
        script.
            window.prerenderReady = false;

            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                            (i[r].q = i[r].q || []).push(arguments)
                        }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                        m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-59984697-1', 'auto');
            ga('send', 'pageview');